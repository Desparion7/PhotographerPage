			<section class="contact">
				<h2>Kontakt</h2>
				<div class="contact-box">
					<div class="contact-method">
						<div class="method-phone">
							<i class="fa-solid fa-phone"></i>+48 575 631 571
						</div>
						<div class="method-email">
							<i class="fa-solid fa-at"></i>E-mail: adrianpruchnik@gmail.com
						</div>
						<a
							href="https://www.facebook.com/adrianpruchnikfotografia"
							target="_blank"
							rel="noopener noreferrer"
							><div class="method-facebook">
								<i class="fa-brands fa-facebook-f"></i>@adrianpruchnikfotografia
							</div></a
						>
					</div>
					<form
						name="form-contact"
						onsubmit="return checkFrom()"
						class="form-contact"
						action="https://formsubmit.io/send/wosmateusz3@gmail.com"
						method="POST"
					>
						<input
							name="_redirect"
							type="hidden"
							id="name"
							value="https://desparion7.github.io/PhotographerPage/potwierdzenie.html"
						/>
						<label for="message">Wiadomość:*</label>
						<textarea
							type="message"
							id="message"
							name="message"
							rows="6"
							required
						></textarea>

						<label for="client-name">Imię:*</label>
						<input
							type="client-name"
							id="client-name"
							name="client-name"
							required
						/>

						<label for="email">E-mail:*</label>
						<input
							type="email "
							id="email"
							name="email"
							placeholder=""
							required
						/>

						<label for="phone">Nr telefonu:*</label>
						<input type="number" id="phone" name="phone" required />
						<input name="_formsubmit_id" type="text" style="display: none" />

						<div class="agreement">
							<input type="radio" name="akcept" required />Wyrażam zgodę na
							przetwarzania powyższych danych osobowych zgodnie z ustawą o
							ochronie danych osobowych*.
						</div>

						<div class="send-btn">
							<input value="Wyślij" type="submit" class="btn" id="send-btn" />
						</div>
					</form>
				</div>
